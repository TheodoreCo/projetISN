INSERT INTO ETAT VALUES(1,'Tu ouvres lentement les yeux. Tes paupi\u00e8res sont lourdes,et tu sens une douleur sourde t''\u00e9treindre le cr\u00e2ne.\u000aTu regardes autour de toi et tu ne reconnais pas l''endroit o\u00f9 tu te trouves. \u000aTu te situes dans une pi\u00e8ce apparement vide, allong\u00e9 sur un lit. Tu fermes les yeux \u00e0 nouveau.\u000aTa t\u00eate te fait attrocement souffrir. \u000aPr\u00e9f\u00e8res tu surmonter ta souffrance et sortir du lit afin d''en savoir plus sur l''endroit o\u00f9 tu te trouves \u000aou bien essaies tu de te rendormir pour r\u00e9cup\u00e9rer des forces?','Point de d\u00e9part ?','mysterious.ogg')
INSERT INTO ETAT VALUES(2,'Tu te r\u00e9veilles. La salle est \u00e9clair\u00e9e par une faible lumiere.\u000aUne porte se trouve sur ta gauche, et une autre sur ta droite. \u000aQue fais tu ?','Je me r\u00e9veille',NULL)
INSERT INTO ETAT VALUES(3,'Ext\u00e9nu\u00e9, tu refermes tes paupi\u00e8res et sombres dans un sommeil de plomb. Tu es r\u00e9v\u00e9ill\u00e9 brusquement par un bruit de pas qui semblent s''avancer vers la chambre dans laquelle tu trouves.\u000aTu regardes autour de toi. Tu te trouves \u00e0 l''int\u00e9rieur d''une petite pi\u00e8ce, \u00e9clair\u00e9e faiblement par une lampe suspendue au plafond.\u000aUne porte se situe \u00e0 ta droite, et une autre \u00e0 ta gauche. Tu remarques que la serrure de la porte droite est rouill\u00e9e. Que fais-tu?','Je me rendors',NULL)
INSERT INTO ETAT VALUES(200,'Suspicieux \u00e0 propos de l''endroit o\u00f9 tu te trouves, tu te caches sous le lit.','Je me cache sous le lit.',NULL)
INSERT INTO ETAT VALUES(201,'Intrigu\u00e9 par ta situation, tu pr\u00e9f\u00e8res attendre et voir comment vont se d\u00e9rouler les \u00e9venements.','Tu restes dans le lit sans rien faire',NULL)
INSERT INTO ETAT VALUES(202,'Effray\u00e9, tu essaies de sortir de cette salle en actionnant la poign\u00e9e de la porte droite.\u000aMalheuresement, cette porte est VEROUILLEE, tu ne sors donc pas de la salle ','Tu essaies de sortir par la porte droite',NULL)
INSERT INTO ETAT VALUES(203,'Effray\u00e9, tu essaies de sortir de cette salle en actionnant la poign\u00e9e de la porte gauche.\u000aLa porte n''\u00e9tant PAS VEROUILLEE, tu sors de la salle.','Tu essaies de sortir par la porte gauche',NULL)
INSERT INTO ETAT VALUES(4,'Tu te diriges vers la porte gauche. Tu tournes la poign\u00e9e et tu sors de la pi\u00e8ce.Puis soudainement tu entends des bruits de pas.','J''essaie de sortir de la piece par la porte gauche',NULL)
INSERT INTO ETAT VALUES(5,'tu rentres dans la chambre pour attendre que les bruits de pas cessent,et tu te retrouves face a un scientifique ','je repars dans la chambre',NULL)
INSERT INTO ETAT VALUES(6,'tu patientes ... pour finalement rencontrer un personnage vetu de blanc imposant qui fait peur a voir ','je patiente',NULL)
INSERT INTO ETAT VALUES(7,'tu prends les escaliers pour t''enfuir','je prends les escaliers',NULL)
INSERT INTO ETAT VALUES(8,'tu te diriges vers les bruits de pas et tu tombes sur un scientifique ','je vois d''o\u00f9 viennent les pas',NULL)
INSERT INTO ETAT VALUES(9,'tu discutes avec le scientifique','tu le trouves sympatique',NULL)
INSERT INTO ETAT VALUES(10,'tu prends la fuite ','tu le trouves effrayant',NULL)
INSERT INTO ETAT VALUES(101,'Tu te penches et tu regardes en dessous du lit. Il n''y a rien. \u000a\u000aPuis soudainement tu entends des bruits de pas!','Je regarde ce qu''il y a en dessous du lit.',NULL)
INSERT INTO ETAT VALUES(102,'Tu te diriges vers la porte droite et tu essaies de tourner la poign\u00e9e. Mais la porte est v\u00e9rouill\u00e9e! Tu remarques une serrure rouil\u00e9e sous la poign\u00e9e.\u000a\u000a Puis soudainement tu entends des bruits de pas! ','je veux sortir par la porte droite.',NULL)
INSERT INTO ETAT VALUES(110,'tu te caches sous le lit','je me cache sous le lit',NULL)
INSERT INTO ETAT VALUES(111,'Tu patientes. Les bruits de pas se rapprochent','Je patiente.',NULL)
INSERT INTO ETAT VALUES(112,'Tu essayes de sortir par la porte droite. Malheuresement, elle est v\u00e9rouill\u00e9e.','Je sorts de la pi\u00e8ce par la porte droite.',NULL)
INSERT INTO ETAT VALUES(113,'Tu reviens dans le lit. Les bruits de pas se rapprochent...','Je reviens \u00e0 l''int\u00e9rieur du lit et je feins le sommeil.',NULL)
INSERT INTO ETAT VALUES(114,'Tu tournes la poign\u00e9e et tu sors de la pi\u00e8ce.','Je sors de la pi\u00e8ce par la porte gauche',NULL)
